{
  "name": "pdf_to_image_service",
  "version": "1.0.0",
  "description": "Transforms PDF's into images",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon ./server/index.js",
    "start": "node ./build/index.js",
