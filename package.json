    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "join-images": "^1.1.3",
    "multer": "^1.4.4",
    "pdf-to-png-converter": "^0.2.2",
    "sharp": "^0.30.4"
  },
  "devDependencies": {
    "@babel/node": "^7.16.8",
